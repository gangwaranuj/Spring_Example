<div class="wm-modal user-profile-popup user-profile" data-modal>
	{{#each this.slides}}
		<div id="{{this.id}}" class="wm-modal--slide {{#if this.isActive}}-active{{/if}}">
			<div class="wm-modal--toolbar">
				<div class="wm-modal--close wm-icon-x" data-modal-close></div>
				<h1 class="wm-modal--header">
					<span class="wm-modal--title"><a href='{{this.title}}'>View Full Profile</a></span>
				</h1>
			</div>
			<div class="wm-modal--content profile"></div>
		</div>
	{{/each}}
</div>
