<div class="wm-pagination" data-min="{{#if min}}{{min}}{{else}}1{{/if}}" data-max="{{max}}">
	<a class="wm-pagination--back wm-icon-left-arrow" href="{{#if previous}}{{previous}}{{else}}javascript:void(0){{/if}}"></a>
	<span class="wm-pagination--label">of</span>
	<a class="wm-pagination--next wm-icon-right-arrow" href="{{#if next}}{{next}}{{else}}javascript:void(0){{/if}}"></a>
</div>
