<td class="actions">
	<input type="checkbox"{{#if this.requirementSet.active}}checked="checked"{{/if}} data-action="power-on"/>
</td>
<td class="name">{{this.requirementSet.name}}</td>
<td class="is-required">
	<input type="checkbox"{{#if this.requirementSet.required}}checked="checked"{{/if}} data-action="require"/>
</td>
<td class="creator">{{this.requirementSet.creatorName}}</td>
<td class="actions">
	{{#if this.requirementSet.groupId}}
		<a href="/groups/{{this.requirementSet.groupId}}/requirements" aria-label="Edit" class="tooltipped tooltipped-n">
			<i class="wm-icon-edit icon-large muted"></i>
		</a>
	{{/if}}
	{{#unless this.requirementSet.groupId}}
		<a class="tooltipped tooltipped-n" href="#edit/{{this.requirementSet.id}}" aria-label="Edit">
			<i class="wm-icon-edit icon-large muted"></i>
		</a>
	{{/unless}}
</td>
<td class="actions">
	<a class="tooltipped tooltipped-n" aria-label="Delete">
		<i class="wm-icon-trash icon-large muted" data-action="trash"></i>
	</a>
</td>
