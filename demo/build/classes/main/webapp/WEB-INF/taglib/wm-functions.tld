<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd" version="2.0">
	<tlib-version>1.0</tlib-version>
	<short-name>wm-fn</short-name>
	<uri>http://www.workmarket.com/taglib/functions</uri>

	<!-- Collections -->
	<function>
		<description>Join elements of a java.util.Collection into a string.</description>
		<name>join</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>java.lang.String join(java.util.Collection, java.lang.String)</function-signature>
	</function>

	<function>
		<description>Join elements of a java.util.Collection into a human-readable string.</description>
		<name>joinHuman</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>java.lang.String joinHuman(java.util.Collection, java.lang.String, java.lang.String)</function-signature>
	</function>

	<function>
		<description>Join a specific property of elements in a java.util.Collection into a string.</description>
		<name>joinProperty</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>java.lang.String join(java.util.Collection, java.lang.String, java.lang.String)</function-signature>
	</function>

	<function>
		<description>Join a specific property of elements in a java.util.Collection into a human-readable string.</description>
		<name>joinPropertyHuman</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>java.lang.String joinHuman(java.util.Collection, java.lang.String, java.lang.String, java.lang.String)</function-signature>
	</function>

	<function>
		<description>Check whether the collection contains the provided object.</description>
		<name>contains</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>boolean containsObject(java.util.Collection,java.lang.Object)</function-signature>
	</function>

	<function>
		<description>Count items in a collection.</description>
		<name>collectionSize</name>
		<function-class>com.workmarket.utility.CollectionUtilities</function-class>
		<function-signature>int collectionSize(java.util.Collection)</function-signature>
	</function>

	<!-- Strings -->
	<function>
		<description>Evaluate condition, return 'yes' if true and 'no' if false.</description>
		<name>toYesNo</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String toYesNo(java.lang.Boolean)</function-signature>
	</function>

	<function>
		<name>defaultString</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String defaultString(java.lang.String,java.lang.String)</function-signature>
	</function>

	<function>
		<name>strip</name>
		<function-class>org.apache.commons.lang3.StringUtils</function-class>
		<function-signature>java.lang.String strip(java.lang.String, java.lang.String)</function-signature>
	</function>

	<function>
		<name>stripUriProtocol</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String stripUriProtocol(java.lang.String)</function-signature>
	</function>

	<function>
		<name>stripTrailingSlashes</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String stripTrailingSlashes(java.lang.String)</function-signature>
	</function>

	<function>
		<name>fullName</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String fullName(java.lang.String,java.lang.String)</function-signature>
	</function>

	<function>
		<name>encrypteId</name>
		<function-class>com.workmarket.utility.EncryptionUtilities</function-class>
		<function-signature>java.lang.String encryptLong(java.lang.Long)</function-signature>
	</function>

	<function>
		<name>urlEncode</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String urlEncode(java.lang.String,java.lang.String)</function-signature>
	</function>

	<function>
		<name>maskDigits</name>
		<function-class>com.workmarket.utility.StringUtilities</function-class>
		<function-signature>java.lang.String maskDigits(java.lang.String, java.lang.String)</function-signature>
	</function>

	<!-- Booleans -->
	<function>
		<description>Evaluate condition and return corresponding true/false string.</description>
		<name>boolean</name>
		<function-class>org.apache.commons.lang.BooleanUtils</function-class>
		<function-signature>java.lang.String toString(boolean, java.lang.String, java.lang.String)</function-signature>
	</function>

	<!-- Numbers -->
	<function>
		<description>If number is null, it will return 0</description>
		<name>nVal</name>
		<function-class>com.workmarket.utility.NumberUtilities</function-class>
		<function-signature>java.lang.Integer getNullSafe(java.lang.Integer)</function-signature>
	</function>

	<function>
		<description>If the value is 0, it will return '-'. Otherwise return value * 100</description>
		<name>scoreCardValueDisplay</name>
		<function-class>com.workmarket.utility.AnalyticsUtilities</function-class>
		<function-signature>java.lang.String scoreCardValueDisplay(java.lang.Double)</function-signature>
	</function>

	<!-- Tags -->
	<function>
		<name>instanceOf</name>
		<function-class>com.workmarket.utility.TagUtilities</function-class>
		<function-signature>boolean instanceOf(java.lang.Object, java.lang.String)</function-signature>
	</function>

	<function>
		<name>hasProperty</name>
		<function-class>com.workmarket.utility.TagUtilities</function-class>
		<function-signature>boolean hasProperty(java.lang.Object, java.lang.String)</function-signature>
	</function>

