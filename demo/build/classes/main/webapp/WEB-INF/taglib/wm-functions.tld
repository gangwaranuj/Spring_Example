	<function>
		<name>ratingStars</name>
		<function-class>com.workmarket.web.helpers.RatingStarsHelper</function-class>
		<function-signature>java.lang.String getStars(double)</function-signature>
	</function>

	<function>
		<name>ratingLevels</name>
		<function-class>com.workmarket.web.helpers.RatingStarsHelper</function-class>
		<function-signature>java.lang.String getLevels(int)</function-signature>
	</function>

	<function>
		<name>ratingCode</name>
		<function-class>com.workmarket.web.helpers.RatingStarsHelper</function-class>
		<function-signature>java.lang.String getRatingCode(int)(int)</function-signature>
	</function>

	<function>
		<name>convertRatingNumberToNumeric</name>
		<function-class>com.workmarket.web.helpers.RatingStarsHelper</function-class>
		<function-signature>int convertRatingNumberToNumeric(double)</function-signature>
	</function>

	<function>
		<name>ratingValueFromShort</name>
		<function-class>com.workmarket.web.helpers.RatingStarsHelper</function-class>
		<function-signature>java.lang.String getRatingValueFromShort(java.lang.Short)</function-signature>
	</function>

	<function>
		<name>translateDeliverableTypeToName</name>
		<function-class>com.workmarket.domains.model.asset.type.WorkAssetAssociationType</function-class>
		<function-signature>java.lang.String translateDeliverableTypeToName(java.lang.String)</function-signature>
	</function>

	<function>
		<name>getEmbeddedMapFromAddress</name>
		<function-class>com.workmarket.utility.GeoUtilities</function-class>
		<function-signature>java.lang.String getEmbeddedMapFromAddress(java.lang.Integer, java.lang.Integer, java.lang.Boolean, java.lang.String, java.lang.Integer, java.lang.Boolean)</function-signature>
	</function>

	<function>
		<name>getStaticMapImageUrlFromAddress</name>
		<function-class>com.workmarket.utility.GeoUtilities</function-class>
		<function-signature>java.lang.String getStaticMapImageUrlFromAddress(java.lang.Integer, java.lang.Integer, java.lang.String, java.lang.Boolean, java.lang.Integer, java.lang.Integer)</function-signature>
	</function>

	<function>
		<name>getMapUrlFromAddress</name>
		<function-class>com.workmarket.utility.GeoUtilities</function-class>
		<function-signature>java.lang.String getMapUrlFromAddress(java.lang.String)</function-signature>
	</function>

	<!-- Mobile -->
	<function>
		<name>getMobileDatePicker</name>
		<function-class>com.workmarket.web.helpers.MobileHelper</function-class>
		<function-signature>java.lang.String getMobileDatePicker(java.lang.String,java.lang.String,java.lang.String,java.lang.String)</function-signature>
	</function>

	<function>
		<name>getMobileTimePicker</name>
		<function-class>com.workmarket.web.helpers.MobileHelper</function-class>
		<function-signature>java.lang.String getMobileTimePicker(java.lang.String,java.lang.String,java.lang.String,java.lang.Integer)</function-signature>
	</function>
	<function>
		<name>getCurrentYear</name>
		<function-class>com.workmarket.utility.DateUtilities</function-class>
		<function-signature>int getCurrentYear()</function-signature>
	</function>

	<function>
		<description>check if 2 dates are on the same day</description>
		<name>isSameDay</name>
		<function-class>com.workmarket.utility.DateUtilities</function-class>
		<function-signature>boolean isSameDay(java.util.Calendar, java.util.Calendar)</function-signature>
	</function>
</taglib>
