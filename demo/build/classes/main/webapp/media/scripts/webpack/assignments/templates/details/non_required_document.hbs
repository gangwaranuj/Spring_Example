					<g opacity="0.3">
						<path d="M16.318,0.687v3.96c0,2.047,1.66,3.707,3.707,3.707H23.5"/>
						<path d="M23.5,8.541h-3.476c-2.147,0-3.894-1.747-3.894-3.894V0.688h0.375v3.959c0,1.94,1.579,3.519,3.519,3.519H23.5V8.541z"/>
					</g>
					<path fill="#FFFFFF" d="M16.818,0.187v3.96c0,2.047,1.66,3.707,3.707,3.707H24"/>
					<path fill="#4C5355" d="M24,8.041h-3.476c-2.147,0-3.894-1.747-3.894-3.894V0.188h0.375v3.959c0,1.94,1.579,3.519,3.519,3.519
								H24V8.041z"/>
					<text fill="#4C5355" font-weight="600" font-family="Open Sans" {{{this.asset.extensionSize}}}>{{this.asset.upperExtension}}</text>
				</g>
			</svg>

			</div>
		{{/unless}}
		<div class="options -hidden">
			{{#if this.isAdminOrActiveResource}}
				{{#if this.asset.isUpload}}
				<a href="/asset/downloadTemp/{{this.asset.uuid}}">
				{{/if}}
				{{#unless this.asset.isUpload}}
				<a href="/asset/download/{{this.asset.uuid}}">
				{{/unless}}
				<svg class="deliverable-download" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.0" x="0px" y="0px" viewBox="0 0 32 24" enable-background="new 0 0 32 24" xml:space="preserve">
					<path d="M29.167 24H2.833C1.269 24 0 22.7 0 21.2V2.8C0 1.3 1.3 0 2.8 0h26.333C30.731 0 32 1.3 32 2.8 v18.4C32 22.7 30.7 24 29.2 24z" class="background"/>
					<path d="M18.025 16l-1.373 1.362c-0.261 0.259-0.608 0.402-0.977 0.4 c-0.369 0-0.716-0.143-0.976-0.401L13.323 16H8v3.528h16V16H18.025z M22.553 18.646h-2.465c-0.3 0-0.543-0.582-0.543-0.882 c0-0.3 0.243-0.882 0.543-0.882h2.465c0.3 0 0.5 0.6 0.5 0.882C23.095 18.1 22.9 18.6 22.6 18.646z" class="foreground"/>
					<path d="M18.331 3h-5.582c-0.423 0-0.766 0.34-0.766 0.759v4.047l-1.677-0.002c-0.854 0-1.145 0.478-1.231 0.7 C8.99 8.7 8.9 9.2 9.5 9.835l5.048 5.002c0.276 0.3 0.6 0.4 1 0.425c0.391 0 0.759-0.151 1.035-0.426 l5.044-5.002c0.603-0.597 0.468-1.14 0.382-1.346c-0.086-0.206-0.378-0.684-1.231-0.684l-1.676 0.002V3.759 C19.096 3.3 18.8 3 18.3 3L18.331 3z" class="foreground"/>
				</svg>
			</a>
				<svg class="remove" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.0" x="0px" y="0px" viewBox="0 0 32 24" enable-background="new 0 0 32 24" xml:space="preserve">
				<path d="M29.167 24H2.833C1.269 24 0 22.7 0 21.2V2.8C0 1.3 1.3 0 2.8 0h26.333C30.731 0 32 1.3 32 2.8 v18.4C32 22.7 30.7 24 29.2 24z" class="background"/>
				<polygon points="19,4 19,3 14,3 14,4 10,4 10,6 23,6 23,4" class="foreground"/>
				<polygon points="11.1,7 12.1,19 20.7,19 21.7,7" class="foreground"/>
			</svg>
			{{/if}}
			{{#unless this.isAdminOrActiveResource}}
				{{#if this.asset.isUpload}}
					<a class="asset-name" href="/asset/downloadTemp/{{this.asset.uuid}}">
				{{/if}}
				{{#unless this.asset.isUpload}}
					<a class="asset-name" href="/asset/download/{{this.asset.uuid}}">
				{{/unless}}
				<svg class="deliverable-download" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.0" x="0px" y="0px" viewBox="0 0 32 24" enable-background="new 0 0 32 24" xml:space="preserve">
					<path d="M29.167 24H2.833C1.269 24 0 22.7 0 21.2V2.8C0 1.3 1.3 0 2.8 0h26.333C30.731 0 32 1.3 32 2.8 v18.4C32 22.7 30.7 24 29.2 24z" class="background"/>
					<path d="M18.025 16l-1.373 1.362c-0.261 0.259-0.608 0.402-0.977 0.4 c-0.369 0-0.716-0.143-0.976-0.401L13.323 16H8v3.528h16V16H18.025z M22.553 18.646h-2.465c-0.3 0-0.543-0.582-0.543-0.882 c0-0.3 0.243-0.882 0.543-0.882h2.465c0.3 0 0.5 0.6 0.5 0.882C23.095 18.1 22.9 18.6 22.6 18.646z" class="foreground"/>
					<path d="M18.331 3h-5.582c-0.423 0-0.766 0.34-0.766 0.759v4.047l-1.677-0.002c-0.854 0-1.145 0.478-1.231 0.7 C8.99 8.7 8.9 9.2 9.5 9.835l5.048 5.002c0.276 0.3 0.6 0.4 1 0.425c0.391 0 0.759-0.151 1.035-0.426 l5.044-5.002c0.603-0.597 0.468-1.14 0.382-1.346c-0.086-0.206-0.378-0.684-1.231-0.684l-1.676 0.002V3.759 C19.096 3.3 18.8 3 18.3 3L18.331 3z" class="foreground"/>
				</svg>
			</a>
			{{/unless}}
		</div>
		<input type="hidden" name="attachments[{{this.asset.index}}].id" value="{{this.asset.id}}"/>
		<input type="hidden" name="attachments[{{this.asset.index}}].uuid" value="{{this.asset.uuid}}"/>
		<input type="hidden" name="attachments[{{this.asset.index}}].name" value="{{this.asset.name}}"/>
		{{#if this.asset.isUpload}}
			<input type="hidden" name="attachments[{{this.asset.index}}].isUpload" value="1"/>
		{{/if}}
		{{#unless this.asset.isUpload}}
			<input type="hidden" name="attachments[{{this.asset.index}}].isUpload" value="0"/>
		{{/unless}}
	</div>
	<div class="asset-info span6">
		<p class="asset-name">{{this.asset.name}} </p>
		<p class="asset-timestamp">{{this.asset.createdOnDate}}</p>
	</div>
	<div class="asset-visibility span6">
		<div class="asset-visibility dropdown tooltipped tooltipped-n" aria-label="{{this.asset.visibilityTypeDescription}}">
			<input class="visibility-selection dn" name="attachments[{{this.asset.index}}].visibilityType"  value="{{this.asset.visibilityCode}}"/>
			<span class="visibility-copy">Visible to:</span>
			<a class="toggle-visibility dropdown-toggle button" data-toggle="dropdown" href="#">
				{{#each this.visibilitySettings}}
					<span class="{{this.code}}">{{> ../../../funcs/templates/icon name=this.icon-code}}</span>
				{{/each}}
			</a>
			<ul class="dropdown-menu">
				{{#each this.visibilitySettings}}
					<li>
						<a class="visibility-option" data-visibility-code="{{this.code}}" data-visibility-description="{{this.description}}">
							<span class="visibility-icon">{{> ../../../funcs/templates/icon name=this.icon-code}}</span>
							<span class="visibility-description">{{this.description}}</span>
							<span class="checkmark"><i class="wm-icon-checkmark"></i></span>
						</a>
					</li>
				{{/each}}
			</ul>
		</div>
	</div>
	<div class="asset-description-container">
		<textarea rows="1" maxlength="512" class="asset-description" placeholder="Short Description (Optional)" name="attachments[{{this.asset.index}}].description">{{this.asset.description}}</textarea>
	</div>
</li>
