<li class="non-required-deliverable">
	<div class="deliverable-icon-container span3">
		{{#if this.asset.containsImageMimeType}}
			{{#if this.asset.isUpload}}
				<div class="deliverable-icon" style="background-image: url('/asset/downloadTemp/{{this.asset.uuid}}');"></div>
			{{/if}}
			{{#unless this.asset.isUpload}}
				<div class="deliverable-icon" style="background-image: url('/asset/{{this.asset.uuid}}');"></div>
			{{/unless}}
		{{/if}}
		{{#unless this.asset.containsImageMimeType}}
			<div class="no-background deliverable-icon">
				<svg version="1.1"  class="deliverables-live-icon icon-position file-type-icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
		 		viewBox="0 0 29 29" enable-background="new 0 0 29 29" xml:space="preserve">
