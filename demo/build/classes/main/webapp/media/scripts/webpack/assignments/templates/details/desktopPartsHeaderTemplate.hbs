		<input maxlength="{{partsConstants.NAME_MAX}}" class="parts-table--item-name" type="text" placeholder="Part Name" />
		<input maxlength="{{partsConstants.TRACKING_NUMBER_MAX}}" class="parts-table--tracking-number-input" type="text" placeholder="Tracking Number"/>
		<input max="{{partsConstants.PART_VALUE_MAX}}" min="{{partsConstants.PART_VALUE_MIN}}" step="0.01" class="parts-table--item-price" placeholder="Part Value" pattern="[0-9$,.]*" type="number" />
		<span class="parts-table--hidden-dollar-sign">$</span>
		<button disabled class="button parts-table--add">Add</button>
